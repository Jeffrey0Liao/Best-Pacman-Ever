            <Font size="20.0" />
         </font></Button>
      <Button fx:id="speedUp" layoutX="689.0" layoutY="447.0" mnemonicParsing="false" onAction="#buySpeed" prefHeight="50.0" prefWidth="280.0" text="Speed Up          $1000">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Label fx:id="yourMoney" layoutX="950.0" layoutY="80.0" text="\$Label" textFill="#c3a40e">
         <font>
            <Font size="50.0" />
         </font>
      </Label>
      <ImageView fitHeight="240.0" fitWidth="280.0" layoutX="252.0" layoutY="196.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fitHeight="240.0" fitWidth="280.0" layoutX="689.0" layoutY="196.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="513.0" layoutY="566.0" mnemonicParsing="false" onAction="#storeGoBack" prefHeight="40.0" prefWidth="200.0" text="Go Back">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
