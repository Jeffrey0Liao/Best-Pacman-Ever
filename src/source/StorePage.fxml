<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="1225.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.StoreController">
   <children>
      <ImageView fitHeight="718.0" fitWidth="1225.0" pickOnBounds="true" preserveRatio="true">
