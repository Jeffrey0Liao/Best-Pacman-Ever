         <font>
            <Font size="45.0" />
         </font>
      </Label>
      <Button fx:id="moreLife" layoutX="255.0" layoutY="447.0" mnemonicParsing="false" onAction="#buyLife" prefHeight="50.0" prefWidth="280.0" text="More Life          $1000">
